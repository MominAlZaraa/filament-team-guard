{
    "name": "mominalzaraa/filament-team-guard",
    "description": "Filament Team Guard â€” enhanced Laravel starter kit built with Filament. Team management, auth, 2FA, passkeys, API tokens. Inspired by Laravel Jetstream.",
    "keywords": [
        "mominalzaraa",
        "laravel",
        "filament",
        "filament-team-guard",
        "teams",
        "multi-tenant",
        "authentication",
        "starter-kit",
        "2fa",
        "passkeys"
    ],
    "homepage": "https://github.com/MominAlZaraa/filament-team-guard",
    "support": {
        "issues": "https://github.com/MominAlZaraa/filament-team-guard/issues",
        "source": "https://github.com/MominAlZaraa/filament-team-guard"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/MominAlZaraa/filament-team-guard.git"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Momin Al Zaraa",
            "email": "support@mominpert.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^5.0",
        "illuminate/contracts": "^12.0",
        "mobiledetect/mobiledetectlib": "^4.8",
        "njoguamos/laravel-turnstile": "^3.2",
        "pragmarx/google2fa": "^8.0",
        "bacon/bacon-qr-code": "^3.0",
        "spatie/laravel-passkeys": "^1.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^8.0",
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "larastan/larastan": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Filament\\Jetstream\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Filament\\Jetstream\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Filament\\Jetstream\\JetstreamServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
